---
import { Header } from '../components/header'
import Layout from '../layouts/Layout.astro'
import '../styles/global.css'
import '../styles/shiny-cta.css'
import FeatureSectionV3 from '../components/feature-section-v3'
import { Faq } from '../components/faq'
import FooterSection from '../components/footer-section-v2'
import { ReactLayout } from '../layouts/react-layout'
import { LaunchAd } from '../components/ads'
import { SolarLayout } from '../components/solar'
import { Partner } from '../components/partner'
import { Funcs } from '../components/funcs'
---

<Layout>
  <ReactLayout client:load>
    <>
      <Header client:load />
      <SolarLayout client:load />
      <FeatureSectionV3 client:load />
      <Funcs client:load />
      <Faq client:load />
      <LaunchAd />
      <FooterSection client:load />
    </>
  </ReactLayout>
</Layout>
